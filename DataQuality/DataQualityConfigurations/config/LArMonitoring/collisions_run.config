dir LAr {

## FEBMon ##

    dir FEBMon {

      dir Summary { 

        algorithm = LAr_GatherData

        hist NbOfEventsVsLB {
             output = LAr/LAR_GLOBAL/Run_Parameters
             display = StatBox
        }
        hist YieldOfOneErrorEventsVsLB {
             output = LAr/LAR_GLOBAL/Data_Integrity
             display = StatBox
             algorithm = LAr_FEBMon_BinsGreaterThan001
