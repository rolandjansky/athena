                    algorithm = GatherData
                }
                hist NoTrig_Segments_Container_BE_etastation_Large {
                    output = MuonTracking/Expert/Segments/Container
                    algorithm = GatherData
                }
                hist NoTrig_Segments_Container_BE_etastation_Small {
                    output = MuonTracking/Expert/Segments/Container
                    algorithm = GatherData
                }
                hist NoTrig_Segments_Container_EI_etastation_Large {
                    output = MuonTracking/Expert/Segments/Container
                    algorithm = GatherData
                }
                hist NoTrig_Segments_Container_EI_etastation_Small {
                    output = MuonTracking/Expert/Segments/Container
                    algorithm = GatherData
                }
                hist NoTrig_Segments_Container_EM_etastation_Large {
