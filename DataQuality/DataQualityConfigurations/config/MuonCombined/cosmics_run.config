                    output = MuonTracking/Shifter/Reco
                    algorithm = MCP_Histogram_Not_Empty&GatherData
                    description = All Segments, midpoint position in small sectors
                    }
                # -----------------------------------------
                hist NoTrig_Segments_Container_xypos_barrel {
                    output = MuonTracking/Expert/Segments/Container
                    algorithm = MCP_Histogram_Not_Empty&GatherData
                    description = for expert
                }
                hist NoTrig_Segments_Container_xypos_endcap {
                    output = MuonTracking/Expert/Segments/Container
                    algorithm = MCP_Histogram_Not_Empty&GatherData
                    description = for expert
                }
                hist NoTrig_Segments_Container_etaphidir {
                    output = MuonTracking/Expert/Segments/Container
                    algorithm = MCP_Histogram_Not_Empty&GatherData
                    description = All Segments, pointing direction in eta and phi
                }
                hist NoTrig_Segments_Container_t0 {
                    output = MuonTracking/Expert/Segments/Container
                    algorithm = MCP_Histogram_Not_Empty&GatherData
                    description = for expert
                }
                hist NoTrig_Segments_Container_t0err {
                    output = MuonTracking/Expert/Segments/Container
                    algorithm = MCP_Histogram_Not_Empty&GatherData
                    description = for expert
                }
                hist NoTrig_Segments_Container_chamberIndex_perSector {
                    output = MuonTracking/Expert/Segments/Container
                    algorithm = MCP_Histogram_Not_Empty&GatherData
                    description = for expert
                }
                hist NoTrig_Segments_Container_eff_chamberIndex_perSector {
                    output = MuonTracking/Expert/Segments/Container
                    algorithm = MCP_Histogram_Not_Empty&GatherData
                    description = for expert
                }
                hist NoTrig_Segments_Container_BI_etastation_Large {
                    output = MuonTracking/Expert/Segments/Container
                    algorithm = GatherData
                }
                hist NoTrig_Segments_Container_BI_etastation_Small {
                    output = MuonTracking/Expert/Segments/Container
                    algorithm = GatherData
                }
                hist NoTrig_Segments_Container_BM_etastation_Large {
