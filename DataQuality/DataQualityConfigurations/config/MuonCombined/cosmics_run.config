############################################################
# MuonCombined
############################################################

#############
# Output
#############

output top_level {

    algorithm = WorstCaseSummary

    output MuonTracking {

        output Shifter {
            
            output Muons {
            }
            output Reco {
            }
        }
        
        output Expert {

            weight = 0
            
            output NonCBMuons {
            }
            output CBMuons {
            }
            output Segments {           
                output AllMuons {
                }
                output Container {
                }
            }
            output TracksMS {
            }
            output TracksID {
            }
            output TracksME {
            }
            output MSVertices {
            }
        }
    }
}

#######################
# Histogram Assessments
#######################


dir MuonPhysics {
    algorithm = MCP_Histogram_Not_Empty&GatherData

    dir NoTrig {

        dir Overview {

            dir CBMuons {
            
                hist NoTrig_Overview_CBMuons_nMuon_LB {
                    output = MuonTracking/Shifter/Muons
                    algorithm = Histogram_Not_Empty
                    description = Number of Muons observed in a LumiBlock
                }
                
            }

            dir NonCBMuons {
                
                hist NoTrig_Overview_NonCBMuons_nMuon_LB {
                    output = MuonTracking/Shifter/Muons
                    algorithm = Histogram_Not_Empty
                    description = Number of Muons observed in a LumiBlock
                }  
                
            }
            
            dir Container {

                hist NoTrig_Overview_Container_nSegment_LB {
                    output = MuonTracking/Shifter/Reco
                    algorithm = Histogram_Not_Empty
                    description = Number of Segments observed in a LumiBlock
                }    
                hist NoTrig_Overview_Container_nMuonTrack_LB {
                    output = MuonTracking/Shifter/Reco
                    algorithm = Histogram_Not_Empty
                    description = Number of Tracks observed in a LumiBlock
                }    
                
            }
        }

        dir Muons {

            dir CBMuons {
        
                hist NoTrig_Muons_CBMuons_pt {
                    output = MuonTracking/Shifter/Muons
                    algorithm = MCP_Histogram_Not_Empty&GatherData
                    description = All Muons, Transverse Momentum
                }
                hist NoTrig_Muons_CBMuons_pt_broad {
                    output = MuonTracking/Shifter/Muons
                    algorithm = GatherData
                    display = AxisRange(100,1000,"X")
                    description = All Muons, Transverse Momentum
                }
                hist NoTrig_Muons_CBMuons_eta_phi_broad {
                    output = MuonTracking/Shifter/Muons
                    algorithm = GatherData
                }
                hist NoTrig_Muons_CBMuons_d0 {
                    output = MuonTracking/Expert/CBMuons
                    algorithm = MCP_Histogram_Not_Empty&GatherData
                    description = All Muons, D0 distribution
                }
                hist NoTrig_Muons_CBMuons_muonType {
                    output = MuonTracking/Expert/CBMuons
                    algorithm = MCP_Histogram_Not_Empty&GatherData
                    description = All Muons, different type distribution
                }
                hist NoTrig_Muons_CBMuons_author {
                    output = MuonTracking/Shifter/Muons
                    algorithm = MCP_Histogram_Not_Empty&GatherData
                    description = All Muons, Algorithms to make those muons
                }
                hist NoTrig_Muons_CBMuons_eta_phi {
                    output = MuonTracking/Shifter/Muons
                    algorithm = MCP_Histogram_Not_Empty&GatherData
                    description = All Muons, Eta and Phi distributions
                }
                hist NoTrig_Muons_CBMuons_ddpt_idme {
                    output = MuonTracking/Expert/CBMuons
                    algorithm = MCP_Histogram_Not_Empty&GatherData
                    description = All Muons, pt difference between ID track and extrapolated track, normalized with ID track pt
                }
                hist NoTrig_Muons_CBMuons_ddpt_cbme {
                    output = MuonTracking/Expert/CBMuons
                    algorithm = MCP_Histogram_Not_Empty&GatherData
                    description = All Muons, pt difference between CB track and extrapolated track, normalized with CB track pt
                }
                hist NoTrig_Muons_CBMuons_chi2ndof {
                    output = MuonTracking/Expert/CBMuons
                    algorithm = MCP_Histogram_Not_Empty&GatherData
                    description = All Muons, chi2 per total degrees of freedom
                }
                hist NoTrig_Muons_CBMuons_tndof {
                    output = MuonTracking/Expert/CBMuons
                    algorithm = MCP_Histogram_Not_Empty&GatherData
                    description = Combined muon total degree of freedom; should agree with reference
                }
                hist NoTrig_Muons_CBMuons_Medium_eff {
                    output = MuonTracking/Shifter/Muons
                    algorithm = MCP_10_events
                    display = AxisRange(0.5,1.0,"Z")
                    description = Combined Muons, medium muon efficiency as a function of eta and phi
                }
                #_---------------------------------------------------
                hist NoTrig_Muons_CBMuons_z0 {
                    output = MuonTracking/Expert/CBMuons
                    algorithm = MCP_Histogram_Not_Empty&GatherData
                    description = for expert
                }
                hist NoTrig_Muons_CBMuons_eta_pt {
                    output = MuonTracking/Expert/CBMuons
                    algorithm = MCP_Histogram_Not_Empty&GatherData
                    description = for expert
                }
                hist NoTrig_Muons_CBMuons_eta {
                    output = MuonTracking/Expert/CBMuons
                    algorithm = MCP_Histogram_Not_Empty&GatherData
                    description = for expert
                }
                hist NoTrig_Muons_CBMuons_phi {
                    output = MuonTracking/Expert/CBMuons
                    algorithm = MCP_Histogram_Not_Empty&GatherData
                    description = for expert
                }
            }

            dir NonCBMuons {
            
                hist NoTrig_Muons_NonCBMuons_pt {
                    output = MuonTracking/Expert/NonCBMuons
                    algorithm = MCP_Histogram_Not_Empty&GatherData
                    description = All Muons, Transverse Momentum
                }
                hist NoTrig_Muons_NonCBMuons_pt_broad {
                    output = MuonTracking/Expert/NonCBMuons
                    algorithm = GatherData
                    display = AxisRange(100,1000,"X")
                    description = All Muons, Transverse Momentum
                }
                hist NoTrig_Muons_NonCBMuons_eta_phi_broad {
                    output = MuonTracking/Expert/NonCBMuons
                    algorithm = GatherData
                }
                hist NoTrig_Muons_NonCBMuons_d0 {
                    output = MuonTracking/Expert/NonCBMuons
                    algorithm = MCP_Histogram_Not_Empty&GatherData
                    description = All Muons, D0 distribution
                }
                hist NoTrig_Muons_NonCBMuons_muonType {
                    output = MuonTracking/Shifter/Muons
                    algorithm = MCP_Histogram_Not_Empty&GatherData
                    description = All Muons, different type distribution
                }
                hist NoTrig_Muons_NonCBMuons_author {
                    output = MuonTracking/Expert/NonCBMuons
                    algorithm = MCP_Histogram_Not_Empty&GatherData
                    description = All Muons, Algorithms to make those muons
                }
                hist NoTrig_Muons_NonCBMuons_eta_phi {
                    output = MuonTracking/Shifter/Muons
                    algorithm = MCP_Histogram_Not_Empty&GatherData
                    description = All Muons, Eta and Phi distributions
                }
                hist NoTrig_Muons_NonCBMuons_chi2ndof {
                    output = MuonTracking/Expert/NonCBMuons
                    algorithm = MCP_Histogram_Not_Empty&GatherData
                    description = All Muons, chi2 per total degrees of freedom
                }
                hist NoTrig_Muons_NonCBMuons_tndof {
                    output = MuonTracking/Expert/NonCBMuons
                    algorithm = MCP_Histogram_Not_Empty&GatherData
                    description = Non combined muon total degree of freedom; should agree with reference
                }
                hist NoTrig_Muons_NonCBMuons_Medium_eff {
                    output = MuonTracking/Expert/NonCBMuons
                    algorithm = MCP_10_events
                    display = AxisRange(0.5,1.0,"Z")
                    description = Non combined Muons, medium muon efficiency as a function of eta and phi
                }
                #_---------------------------------------------------
                hist NoTrig_Muons_NonCBMuons_z0 {
                    output = MuonTracking/Expert/NonCBMuons
                    algorithm = MCP_Histogram_Not_Empty&GatherData
                    description = for expert
                }
                hist NoTrig_Muons_NonCBMuons_eta_pt {
                    output = MuonTracking/Expert/NonCBMuons
                    algorithm = MCP_Histogram_Not_Empty&GatherData
                    description = for expert
                }
                hist NoTrig_Muons_NonCBMuons_eta {
                    output = MuonTracking/Expert/NonCBMuons
                    algorithm = MCP_Histogram_Not_Empty&GatherData
                    description = for expert
                }
                hist NoTrig_Muons_NonCBMuons_phi {
                    output = MuonTracking/Expert/NonCBMuons
                    algorithm = MCP_Histogram_Not_Empty&GatherData
                    description = for expert
                }
            }
        }

        dir TracksID {

            dir CBMuons {
            
                hist NoTrig_TracksID_CBMuons_pt {
                    output = MuonTracking/Expert/TracksID
                    algorithm = MCP_Histogram_Not_Empty&GatherData
                    description = transverse momentum of Inner Detector tracks associated with muons

                }
                hist NoTrig_TracksID_CBMuons_pt_broad {
                    output = MuonTracking/Expert/TracksID
                    algorithm = GatherData
                    display = AxisRange(100,1000,"X")
                    description = transverse momentum of Inner Detector tracks associated with muons

                }
                hist NoTrig_TracksID_CBMuons_eta_phi_broad {
                    output = MuonTracking/Expert/TracksID
                    algorithm = GatherData

                }
                hist NoTrig_TracksID_CBMuons_eta_phi {
                    output = MuonTracking/Expert/TracksID
                    algorithm = MCP_Histogram_Not_Empty&GatherData
                    description = eta-phi occupancy for Inner Detector tracks associated with muons

                }
                hist NoTrig_TracksID_CBMuons_chi2ndof {
                    output = MuonTracking/Shifter/Muons
                    algorithm = MCP_Histogram_Not_Empty&GatherData
                    description = chi square per number of degree of Inner Detector tracks associated with muons

                }
                hist NoTrig_TracksID_CBMuons_HitContent_NBlayerHits {
                    output = MuonTracking/Shifter/Muons
                    algorithm = MCP_Histogram_Not_Empty&GatherData
                    description = B layer hits number of Inner Detector tracks associated with muons

                }
                hist NoTrig_TracksID_CBMuons_HitContent_NPixelHits {
                    output = MuonTracking/Shifter/Muons
                    algorithm = MCP_Histogram_Not_Empty&GatherData
                    description = Pixel hits number of Inner Detector tracks associated with muons

                }
                hist NoTrig_TracksID_CBMuons_HitContent_NSCTHits {
                    output = MuonTracking/Shifter/Muons
                    algorithm = MCP_Histogram_Not_Empty&GatherData
                    description = SCT hits number of Inner Detector tracks associated with muons

                }
                hist NoTrig_TracksID_CBMuons_HitContent_NTRTHits {
                    output = MuonTracking/Shifter/Muons
                    algorithm = MCP_Histogram_Not_Empty&GatherData
                    description = TRT hits number of Inner Detector tracks associated with muons

                }
                hist NoTrig_TracksID_CBMuons_HitContent_NPixelHoles {
                    output = MuonTracking/Expert/TracksID
                    algorithm = MCP_Histogram_Not_Empty&GatherData
                    description = for expert
                }
                hist NoTrig_TracksID_CBMuons_HitContent_NSCTHoles {
                    output = MuonTracking/Expert/TracksID
                    algorithm = MCP_Histogram_Not_Empty&GatherData
                    description = for expert
                }
            }

            dir NonCBMuons {
                
                hist NoTrig_TracksID_NonCBMuons_pt {
                    output = MuonTracking/Expert/TracksID
                    algorithm = MCP_Histogram_Not_Empty&GatherData
                    description = transverse momentum of Inner Detector tracks associated with muons

                }
                hist NoTrig_TracksID_NonCBMuons_pt_broad {
                    output = MuonTracking/Expert/TracksID
                    algorithm = GatherData
                    display = AxisRange(100,1000,"X")
                    description = transverse momentum of Inner Detector tracks associated with muons

                }
                hist NoTrig_TracksID_NonCBMuons_eta_phi_broad {
                    output = MuonTracking/Expert/TracksID
                    algorithm = GatherData

                }
                hist NoTrig_TracksID_NonCBMuons_eta_phi {
                    output = MuonTracking/Expert/TracksID
                    algorithm = MCP_Histogram_Not_Empty&GatherData
                    description = eta-phi occupancy for Inner Detector tracks associated with muons

                }
                hist NoTrig_TracksID_NonCBMuons_chi2ndof {
                    output = MuonTracking/Expert/TracksID
                    algorithm = MCP_Histogram_Not_Empty&GatherData
                    description = chi square per number of degree of Inner Detector tracks associated with muons

                }
                hist NoTrig_TracksID_NonCBMuons_HitContent_NBlayerHits {
                    output = MuonTracking/Expert/TracksID
                    algorithm = MCP_Histogram_Not_Empty&GatherData
                    description = B layer hits number of Inner Detector tracks associated with muons

                }
                hist NoTrig_TracksID_NonCBMuons_HitContent_NPixelHits {
                    output = MuonTracking/Expert/TracksID
                    algorithm = MCP_Histogram_Not_Empty&GatherData
                    description = Pixel hits number of Inner Detector tracks associated with muons

                }
                hist NoTrig_TracksID_NonCBMuons_HitContent_NSCTHits {
                    output = MuonTracking/Expert/TracksID
                    algorithm = MCP_Histogram_Not_Empty&GatherData
                    description = SCT hits number of Inner Detector tracks associated with muons

                }
                hist NoTrig_TracksID_NonCBMuons_HitContent_NTRTHits {
                    output = MuonTracking/Expert/TracksID
                    algorithm = MCP_Histogram_Not_Empty&GatherData
                    description = TRT hits number of Inner Detector tracks associated with muons

                }
                hist NoTrig_TracksID_NonCBMuons_HitContent_NPixelHoles {
                    output = MuonTracking/Expert/TracksID
                    algorithm = MCP_Histogram_Not_Empty&GatherData
                    description = for expert
                }
                hist NoTrig_TracksID_NonCBMuons_HitContent_NSCTHoles {
                    output = MuonTracking/Expert/TracksID
                    algorithm = MCP_Histogram_Not_Empty&GatherData
                    description = for expert
                }
            }
        }

        dir TracksMS {

            dir Container {
            
                hist NoTrig_TracksMS_Container_z0 {
                    output = MuonTracking/Expert/TracksMS
                    display   = Draw=E1,<AxisRange(-20,20,"X")
                    algorithm = MCP_Histogram_Not_Empty&GatherData
                    description = for expert
                }
                hist NoTrig_TracksMS_Container_d0 {
                    output = MuonTracking/Expert/TracksMS
                    display   = Draw=E1,<AxisRange(-0.5,0.5,"X")
                    algorithm = MCP_Histogram_Not_Empty&GatherData
                    description = for expert
                }               
                hist NoTrig_TracksMS_Container_Global_residuals {
                    output = MuonTracking/Expert/TracksMS
                    display   = Draw=E1,<AxisRange(-5,5,"X")
                    algorithm = MCP_Histogram_Not_Empty&GatherData
                    description = for expert
                }
                hist NoTrig_TracksMS_Container_pt {
                    output = MuonTracking/Shifter/Reco
                    display   = Draw=E1,<AxisRange(0,100,"X")>
                    algorithm = MCP_Histogram_Not_Empty&GatherData
                    description = for expert
                }
                hist NoTrig_TracksMS_Container_pt_broad {
                    output = MuonTracking/Shifter/Reco
                    display   = Draw=E1,<AxisRange(100,1000,"X")>
                    algorithm = GatherData
                    description = for expert
                }
                hist NoTrig_TracksMS_Container_eta_phi_broad {
                    output = MuonTracking/Shifter/Reco
                    algorithm = GatherData
                    description = for expert
                }
                hist NoTrig_TracksMS_Container_eta_phi {
                    output = MuonTracking/Shifter/Reco
                    algorithm = MCP_Histogram_Not_Empty&GatherData
                    description = for expert
                }
                hist NoTrig_TracksMS_Container_chi2ndof {
                    output = MuonTracking/Expert/TracksMS
                    algorithm = MCP_Histogram_Not_Empty&GatherData
                    description = for expert
                }
            }

        }

        dir TracksME {     

            dir CBMuons {
        
                hist NoTrig_TracksME_CBMuons_pt {
                        output = MuonTracking/Expert/TracksME
                        display   = Draw=E1,<AxisRange(0,100,"X")
                        algorithm = MCP_Histogram_Not_Empty&GatherData
                        description = for expert
                }
                hist NoTrig_TracksME_CBMuons_pt_broad {
                        output = MuonTracking/Expert/TracksME
                        algorithm = GatherData
                        display = AxisRange(100,1000,"X")
                        description = for expert
                }
                hist NoTrig_TracksME_CBMuons_eta_phi_broad {
                        output = MuonTracking/Expert/TracksME
                        algorithm = GatherData
                        description = for expert
                }
                hist NoTrig_TracksME_CBMuons_eta_phi {
                        output = MuonTracking/Expert/TracksME
                        algorithm = MCP_Histogram_Not_Empty&GatherData
                        description = for expert
                }
                hist NoTrig_TracksME_CBMuons_chi2ndof {
                        output = MuonTracking/Shifter/Muons
                        algorithm = MCP_Histogram_Not_Empty&GatherData
                        description = for expert
                }
                hist NoTrig_TracksME_CBMuons_tndof {
                        output = MuonTracking/Expert/TracksME
                        algorithm = MCP_Histogram_Not_Empty&GatherData
                        description = for expert
                }
                hist NoTrig_TracksME_CBMuons_z0 {
                        output = MuonTracking/Expert/TracksME
                        display   = Draw=E1,<AxisRange(-20,20,"X")
                        algorithm = MCP_Histogram_Not_Empty&GatherData
                        description = for expert
                }   
                hist NoTrig_TracksME_CBMuons_d0 {
                        output = MuonTracking/Expert/TracksME
                        display   = Draw=E1,<AxisRange(-0.5,0.5,"X")
                        algorithm = MCP_Histogram_Not_Empty&GatherData
                        description = for expert
                }               
                hist NoTrig_TracksME_CBMuons_Global_residuals {
                    output = MuonTracking/Expert/TracksME
                    display   = Draw=E1,<AxisRange(-5,5,"X")
                    algorithm = MCP_Histogram_Not_Empty&GatherData
                    description = for expert
                }
            }

        }

        dir MSVertices {

            hist NoTrig_MSVertices_m_MSVx_nMDT {
                output = MuonTracking/Expert/MSVertices
            }
            hist NoTrig_MSVertices_m_MSVx_nRPC {
                output = MuonTracking/Expert/MSVertices
            }
            hist NoTrig_MSVertices_m_MSVx_nTGC {
                output = MuonTracking/Expert/MSVertices
            }
            hist NoTrig_MSVertices_m_MSVx_nTracklets {
                output = MuonTracking/Expert/MSVertices
            }
            hist NoTrig_MSVertices_m_VertexEtaPhi {
                output = MuonTracking/Expert/MSVertices
            }
        }

        dir Segments {

            dir Container {
        
                hist NoTrig_Segments_Container_segmentfitChi2oNdof {
                    output = MuonTracking/Expert/Segments/Container
                    algorithm = MCP_Histogram_Not_Empty&GatherData
                    description = All Segments, Fit Chi2 per Degree of Freedom
                    display = AxisRange(0,12,"X")
                    }
                hist NoTrig_Segments_Container_rzpos_sectorLarge {
                    output = MuonTracking/Shifter/Reco
