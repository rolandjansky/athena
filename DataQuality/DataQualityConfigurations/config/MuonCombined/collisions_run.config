# Thresholds
##############

thresholds MCP_LB_thresholds {
  limits NBins {
    warning = 1400
    error = 1550
  }
}

thresholds MCP_MEtrack_CBMuons_chi2ndof_thresholds {
  limits AbsXMean {
    warning = 1.1
    error = 1.2
  }
}

thresholds MCP_MEtrack_Jpsi_chi2ndof_thresholds {
  limits AbsXMean {
    warning = 0.85
    error = 0.9
  }
}

thresholds MCP_MEtrack_Z_chi2ndof_thresholds {
  limits AbsXMean {
    warning = 0.75
    error = 0.8
  }
}

thresholds MCP_chi2ndof_thresholds {
  limits AbsXMean {
    warning = 0.89
    error = 0.86
  }
}

thresholds MCP_etaphi_thresholds {
  limits AbsXMean {
    warning = 0.3
    error = 0.5
  }
}

thresholds MCP_dpt_thresholds {
  limits AbsXMean {
    warning = 0.1
    error = 0.2
  }
}

thresholds MCP_d0_thresholds {
  limits AbsXMean {
    warning = 0.1
    error = 0.3
  }
}

thresholds MCP_hits_blayer_thresholds {
