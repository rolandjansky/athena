#######################
# Output
#######################

output top_level {
  algorithm = TRMUO_WorstCaseSummary
#tomoe added 2011.1.22
  output HLT {

    output TRMUO {
       output Shift {
         algorithm = TRMUO_WorstCaseSummary
    
         output muChain {
          algorithm = TRMUO_WorstCaseSummary
         }
         output muChainEFIso {
          algorithm = TRMUO_WorstCaseSummary
         }
         output muChainMSonly {
          algorithm = TRMUO_WorstCaseSummary
         }
         output muChainEFFS {
          algorithm = TRMUO_WorstCaseSummary
         }
         output Etaphi {
          algorithm = TRMUO_WorstCaseSummary
         }
       } ##end output Shift
    
       output Expert {
         algorithm = TRMUO_AlwaysGreenSummary
    
        output Efficiency {
          algorithm = TRMUO_WorstCaseSummary
    
          output muChain {
            algorithm = TRMUO_WorstCaseSummary
            output wrtUpstream {
              algorithm = TRMUO_SimpleSummary
            }
            output wrtOffline {
              algorithm = TRMUO_SimpleSummary
            }
          }
          output muChainEFIso {
            algorithm = TRMUO_WorstCaseSummary
            output wrtUpstream {
              algorithm = TRMUO_SimpleSummary
            }
            output wrtOffline {
              algorithm = TRMUO_SimpleSummary
            }
          }
          output muChainMSonly {
            algorithm = TRMUO_WorstCaseSummary
            output wrtUpstream {
              algorithm = TRMUO_SimpleSummary
            }
            output wrtOffline {
              algorithm = TRMUO_SimpleSummary
            }
          }
          output muChainEFFS {
            algorithm = TRMUO_WorstCaseSummary
            output wrtUpstream {
              algorithm = TRMUO_SimpleSummary
            }
            output wrtOffline {
              algorithm = TRMUO_SimpleSummary
            }
          }
        }
        output L1Muon {
          algorithm = TRMUO_WorstCaseSummary
        }
        output L2MuonSA {
          algorithm = TRMUO_WorstCaseSummary
        }
        output L2MuonSAIO {
          algorithm = TRMUO_WorstCaseSummary
        }
        output L2muComb {
          algorithm = TRMUO_WorstCaseSummary
        }
        output EFMuon {
          algorithm = TRMUO_WorstCaseSummary
        }
        output Ratio {
          algorithm = TRMUO_SimpleSummary
        }
    
       } ##end output Expert
    }  #end output TRMUO
 } ## end output HLT
} ## end output top_level



#######################
# Histogram Assessments
#######################

#Example structure
dir HLT {
  algorithm = HLT_Histogram_Not_Empty&GatherData

  dir MuonMon {
    algorithm = Muon_Bins_LessThan_Threshold&GatherData
    output = HLT/TRMUO

    # L2MuonSA
    dir L2MuonSA {
      dir HLT_mu24_ivarmedium_L1MU14FCH {
        hist L2MuonSA_MDT_Mid_residual_barrel {
          output = HLT/TRMUO/Expert/L2MuonSA
          algorithm = alg_L2MuonSA_residual_barrel
          weight = 0.0
        }
        hist L2MuonSA_MDT_Mid_residual_endcap {
          output = HLT/TRMUO/Expert/L2MuonSA
          algorithm = alg_L2MuonSA_residual_endcap
          weight = 0.0
        }
        hist L2MuonSA_Eta_vs_Phi {
          output = HLT/TRMUO/Expert/L2MuonSA
          algorithm = alg_eta_vs_phi
        }
        hist L2MuonSA_proc_flow {
          output = HLT/TRMUO/Expert/L2MuonSA
          algorithm = TRMUO_GatherData
        }
        hist L2MuonSA_Pt {
          output = HLT/TRMUO/Expert/L2MuonSA
          algorithm = TRMUO_GatherData
        }
        hist L2MuonSA_Eta {
          output = HLT/TRMUO/Expert/L2MuonSA
          algorithm = TRMUO_GatherData
        }
        hist L2MuonSA_Phi {
          output = HLT/TRMUO/Expert/L2MuonSA
          algorithm = TRMUO_GatherData
        }
        hist L2MuonSA_Phi_Barrel {
          output = HLT/TRMUO/Expert/L2MuonSA
          algorithm = TRMUO_GatherData
        }
        hist L2MuonSA_Phi_Endcap {
          output = HLT/TRMUO/Expert/L2MuonSA
          algorithm = TRMUO_GatherData
        }
        hist L2MuonSA_MDT_Mid_residual_barrel_OffMatch {
          output = HLT/TRMUO/Expert/L2MuonSA
          algorithm = TRMUO_GatherData
        }
        hist L2MuonSA_MDT_Mid_residual_endcap_OffMatch {
          output = HLT/TRMUO/Expert/L2MuonSA
          algorithm = TRMUO_GatherData
        }
      }
    }

    # L2MuonSA IO
    dir L2MuonSAIO {
      dir HLT_mu24_ivarmedium_mu6_l2io_probe_L1MU14FCH {
        hist EffL2InsideOut_offPt {
          output = HLT/TRMUO/Expert/L2MuonSAIO
          algorithm = TRMUO_GatherData
        }
        hist EffL2InsideOut_offdR {
          output = HLT/TRMUO/Expert/L2MuonSAIO
          algorithm = TRMUO_GatherData
        }
      }
      dir HLT_mu4_l2io_L1MU3V {
        hist L2CBIO_Eta {
          output = HLT/TRMUO/Expert/L2MuonSAIO
          algorithm = TRMUO_GatherData
        }
        hist L2MuonSAIO_distance_bw_FTFroad_and_offlinesegment_Inn_barrel_pT4_6 {
          output = HLT/TRMUO/Expert/L2MuonSAIO
          algorithm = TRMUO_GatherData
        }
        hist L2MuonSAIO_distance_bw_FTFroad_and_offlinesegment_Inn_endcap_pT4_6 {
          output = HLT/TRMUO/Expert/L2MuonSAIO
          algorithm = TRMUO_GatherData
        }
        hist L2CBIO_ptresol_toOffl_neg {
          output = HLT/TRMUO/Expert/L2MuonSAIO
          algorithm = TRMUO_GatherData 
        }
        hist L2CBIO_ptresol_toOffl_pos {
          output = HLT/TRMUO/Expert/L2MuonSAIO
          algorithm = TRMUO_GatherData
        }
        hist L2MuonSAIO_superpoint_multiplicity_pT4_6 {
          output = HLT/TRMUO/Expert/L2MuonSAIO
          algorithm = TRMUO_GatherData
        }
        hist L2MuonSAIO_superpoint_reconstruction_efficiency_wrt_offlinesegment_pT4_6 {
          output = HLT/TRMUO/Expert/L2MuonSAIO
          algorithm = TRMUO_GatherData
        }
      }
    }

    # L2muComb 
    dir L2muComb {
      dir HLT_mu24_ivarmedium_L1MU14FCH {
        hist L2muComb_ptratio_toSA {
          output = HLT/TRMUO/Expert/L2muComb
          algorithm = alg_muComb_ptratio
          weight = 0.0
        }
        hist L2muComb_Eta_vs_Phi {
          output = HLT/TRMUO/Expert/L2muComb
          algorithm = alg_eta_vs_phi
        }
        hist L2muComb_dR_toSA {
          output = HLT/TRMUO/Expert/L2muComb
          algorithm = TRMUO_GatherData
        }
        hist L2muComb_Pt {
          output = HLT/TRMUO/Expert/L2muComb
          algorithm = TRMUO_GatherData
        }
        hist L2muComb_Eta {
          output = HLT/TRMUO/Expert/L2muComb
          algorithm = TRMUO_GatherData
        }
        hist L2muComb_Phi {
          output = HLT/TRMUO/Expert/L2muComb
          algorithm = TRMUO_GatherData
        }
        hist L2muComb_Eta_vs_Phi_L2CBOFFmatching_failure {
          output = HLT/TRMUO/Expert/L2muComb
          algorithm = TRMUO_GatherData
        }
      }
    }

    # EFMuon
    dir EFMuon {
      dir HLT_mu24_ivarmedium_L1MU14FCH {
        hist EFSA_Eta_vs_Phi {
          output = HLT/TRMUO/Expert/EFMuon
          algorithm = alg_eta_vs_phi
        }
        hist EFCB_Eta_vs_Phi {
          output = HLT/TRMUO/Expert/EFMuon
          algorithm = alg_eta_vs_phi
        }
        hist EFSA_Pt {
          output = HLT/TRMUO/Expert/EFMuon
          algorithm = TRMUO_GatherData
          weight = 0.0
        }
        hist EFCB_Pt {
          output = HLT/TRMUO/Expert/EFMuon
          algorithm = TRMUO_GatherData
          weight = 0.0
        }
      }
    }

