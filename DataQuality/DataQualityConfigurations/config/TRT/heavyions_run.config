thresholds TRT_SideBand_Relative {
  thresholds WireToTrkPosition {
    limits Threshold {
      warning = 0.1
      error = 0.2
    }
  }
  thresholds TronTDist {
    limits Threshold {
      warning = 0.05
      error = 0.1
    }
  }
