        }
        hist MuonRoI_Eff_Eta_wBW3CoinVeto_Thr.* {
          output = MuonDetectors/L1MU/L1MUE/Expert/MuonRoIs/Flags/BW3CoinFlag/wBW3CoinVeto
        }
        hist MuonRoI_Eff_Phi_TGC_wBW3CoinVeto_Thr.* {
          output = MuonDetectors/L1MU/L1MUE/Expert/MuonRoIs/Flags/BW3CoinFlag/wBW3CoinVeto
        }
        hist MuonRoI_Eff_Pt_TGC_BW3CoinVeto {
