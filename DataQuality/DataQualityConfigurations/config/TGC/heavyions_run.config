###########################################################
# TGC
###########################################################

#############
# Output
#############

output top_level {
  output MuonDetectors {
    
########################
# Muon Lv1
    output L1MU {
      output L1MUE {
        algorithm = TGC_WorstCaseSummary
        
  # Endcap Lv1 Shift
        output Shift {
          algorithm = TGC_WorstCaseSummary
          output TGCEA {
            algorithm = TGC_WorstCaseSummary
            output Coverage {
              algorithm = TGC_WorstCaseSummary
            }
            output Efficiency {
              algorithm = TGC_WorstCaseSummary
            }
            output Timing {
              algorithm = TGC_WorstCaseSummary
            }
          }
          output TGCEC {
            algorithm = TGC_WorstCaseSummary
            output Coverage {
              algorithm = TGC_WorstCaseSummary
            }
            output Efficiency {
              algorithm = TGC_WorstCaseSummary
            }
            output Timing {
              algorithm = TGC_WorstCaseSummary
            }
          }
        } # Shift
        
  # Endcap Lv1 Expert
        output Expert {
          algorithm = TGC_WorstCaseSummary
          output Global_Summary {
            algorithm = TGC_WorstCaseSummary
          }
          
          output TGCEA {
            algorithm = TGC_WorstCaseSummary
            output Coverage {
              algorithm = TGC_WorstCaseSummary
            }
            output Efficiency {
              algorithm = TGC_WorstCaseSummary
            }
            output Timing {
              algorithm = TGC_WorstCaseSummary
            }
          }
          
          output TGCEC {
            algorithm = TGC_WorstCaseSummary
            output Coverage {
              algorithm = TGC_WorstCaseSummary
            }
            output Efficiency {
              algorithm = TGC_WorstCaseSummary
            }
            output Timing {
              algorithm = TGC_WorstCaseSummary
            }
          }
          
        } # Expert
        
      } # L1MUE
    } # L1MU
    
########################
# MDTvsTGC
    output MDTvsTGC {
      algorithm = TGC_WorstCaseSummary
      output TGCEA {
        algorithm = TGC_WorstCaseSummary
      }
      output TGCEC {
        algorithm = TGC_WorstCaseSummary
      }
    } # MDTvsTGC
    
########################
# TGCraw
    output TGC {
      algorithm = TGC_WorstCaseSummary
      
  # TGCRaw Shift
      output Shift {
        algorithm = TGC_WorstCaseSummary
        output TGCEA {
          algorithm = TGC_WorstCaseSummary
        }
        output TGCEC {
          algorithm = TGC_WorstCaseSummary
        }
      } # Shift
      
  # TGCRaw Expert
      output Expert {
        algorithm = TGC_WorstCaseSummary
        output Global_Summary {
          algorithm = TGC_WorstCaseSummary
        }
        
        output TGCEA {
          algorithm = TGC_WorstCaseSummary
          output Coverage {
            algorithm = TGC_WorstCaseSummary
          }
          output Efficiency {
            algorithm = TGC_WorstCaseSummary
          }
        }
        
        output TGCEC {
          algorithm = TGC_WorstCaseSummary
          output Coverage {
            algorithm = TGC_WorstCaseSummary
          }
          output Efficiency {
            algorithm = TGC_WorstCaseSummary
          }
        }
        
      } # Expert
      
    } # TGCraw
    
    
  } ##MuonDetectors  
}

