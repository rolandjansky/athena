##############
# Algorithms
##############
# Your algorithms; NOT those appearing in the corresponding config/common/ file

# Summary makers

algorithm BinwiseSummaryCaloMon {
  libname = libdqm_summaries.so
  name = BinwiseSummary
}

algorithm WorstCaseYellowCaloMon {
  libname = libdqm_summaries.so
  name = WorstCaseYellow
}

algorithm CaloMon_GatherData {
  libname = libdqm_algorithms.so
  name = GatherData
}

algorithm CaloMon_BinsDiffFromStripMedian {
  libname = libdqm_algorithms.so
  SuppressFactor = 0
  SuppressRedFactor = 0
  name = BinsDiffFromStripMedian
  thresholds = CaloMon_BinsDiffFromStripMedian_threshold
  MaxPublish = 200
}

algorithm CaloMon_BinsDiffFromStripMedian_barrel {
  libname = libdqm_algorithms.so
  xmin = -1.5
  xmax = 1.5
  SuppressFactor = 0
  SuppressRedFactor = 0
  name = BinsDiffFromStripMedian
  thresholds = CaloMon_BinsDiffFromStripMedian_threshold 
  MaxPublish = 200
}

algorithm CaloMon_BinsDiffFromStripMedian_endcapA {
  libname = libdqm_algorithms.so
  xmin = 1.5
  xmax = 4.9
  SuppressFactor = 0
  SuppressRedFactor = 0
  name = BinsDiffFromStripMedian
  thresholds = CaloMon_BinsDiffFromStripMedian_threshold
  MaxPublish = 200
}

algorithm CaloMon_BinsDiffFromStripMedian_endcapC {
  libname = libdqm_algorithms.so
  xmin = -4.9
  xmax = -1.5
  SuppressFactor = 0
  SuppressRedFactor = 0
  name = BinsDiffFromStripMedian
  thresholds = CaloMon_BinsDiffFromStripMedian_threshold
  MaxPublish = 200
}

##############
# Thresholds
##############

# Your thresholds

thresholds CaloMon_BinsDiffFromStripMedian_threshold {
  limits MaxDeviation {
    warning = 25 
    error = 200
  }
}

