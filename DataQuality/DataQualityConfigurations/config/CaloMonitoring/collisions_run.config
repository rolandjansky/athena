#       Undefined
# tmp change
 dir CaloMonitoring {

  dir ClusterMon {  

      dir CaloCalTopoClustersNoTrigSel {
#       output = CaloMonitoring/CaloMonExpert/CaloCalTopoCluster/ClustersForExpert
#       algorithm = CaloMon_GatherData 
       dir Summary {
          hist nEvtsRejectByDifferentTool {
           output = CaloMonitoring/CaloMonExpert/CaloCalTopoCluster/ClustersForExpert/General
           algorithm = CaloMon_GatherData 
          }
       }
       dir General {
          hist etaphi_ncellinclus {
           output = CaloMonitoring/CaloMonExpert/CaloCalTopoCluster/ClustersForExpert/General
           algorithm = CaloMon_BinsDiffFromStripMedian
           display = LogZ
          }
          hist etaphi_ncellinclus@BAR {
           output = CaloMonitoring/CaloMonShift/CaloMonBAR/CaloTopoClustersBAR 
           algorithm = CaloMon_BinsDiffFromStripMedian_barrel
           display = AxisRange(-1.5,1.5,"X"),LogZ
           weight = 0.0
          }
          hist etaphi_ncellinclus@ECA {
           output = CaloMonitoring/CaloMonShift/CaloMonECA/CaloTopoClustersECA 
           algorithm = CaloMon_BinsDiffFromStripMedian_endcapA
           display = AxisRange(1.5,4.9,"X"),LogZ
           weight = 0.0
          }
          hist etaphi_ncellinclus@ECC {
           output = CaloMonitoring/CaloMonShift/CaloMonECC/CaloTopoClustersECC 
           algorithm = CaloMon_BinsDiffFromStripMedian_endcapC
           display = AxisRange(-4.9,-1.5,"X"),LogZ
           weight = 0.0
          }
          
          ##########
          # start TileCal histograms 
          hist TileCalXclustersXE {
           output = CaloMonitoring/CaloMonExpert/CaloCalTopoCluster/ClustersForExpert/TileHistograms
           algorithm = CaloMon_GatherData 
           display = StatBox 
          }
