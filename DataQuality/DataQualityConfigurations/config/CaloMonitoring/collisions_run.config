           output = CaloMonitoring/CaloMonExpert/CaloCalTopoCluster/ClustersForExpert/TileHistograms
           algorithm = CaloMon_GatherData
           display = StatBox 
          }
          hist TileCalXRXE {
            output = CaloMonitoring/CaloMonExpert/CaloCalTopoCluster/ClustersForExpert/TileHistograms
            algorithm = CaloMon_GatherData
            display = StatBox 
           }
          hist TileCalXRXEta {
            output = CaloMonitoring/CaloMonExpert/CaloCalTopoCluster/ClustersForExpert/TileHistograms
            algorithm = CaloMon_GatherData
            display = StatBox 
           }
          hist ClustersXRXPhi {
            output = CaloMonitoring/CaloMonExpert/CaloCalTopoCluster/ClustersForExpert/TileHistograms
            algorithm = CaloMon_GatherData
