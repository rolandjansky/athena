

pcmFN := ${libprefix}${dict}Dict_${NAME}_gen_rdict.pcm


${NAME}.install_lcgdict_pcms :  ${installDir}${pcmFN}
	@echo ${pcmFN} installed


${installDir}${pcmFN} : ${bin}dict/${dict}/${pcmFN}
	@echo "Installing PCM  ${pcmFN}"
#	$(symlink_command) $< $@   # MN: need to get the correct dst path good for symlink ($bin is relative)
	$(atlas_install_command)  $<  $@ 



clean :: ${CONSTITUENT}clean
	@echo "${CONSTITUENT}clean  done"

${CONSTITUENT}clean::
	-rm -f ${installDir}${pcmFN}
