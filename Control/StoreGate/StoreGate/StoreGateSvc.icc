const T* StoreGateSvc::tryConstRetrieve (const TKEY& key) const
{
  if (m_storeID == StoreID::EVENT_STORE && s_pSlot != nullptr) {
    rememberBadRetrieve (ClassID_traits<T>::ID(), key);
