std::unique_ptr<T> 
StoreGateSvc::readUniquePrivateCopy (const std::string& key) {
  return std::unique_ptr<T>(SG::Storable_cast<T>(
                                                 this->typeless_readPrivateCopy(ClassID_traits<T>::ID(), key)));
}


template <typename T>
