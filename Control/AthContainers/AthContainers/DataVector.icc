template <class T, class BASE>
const DataModel_detail::DVLInfoBase& DataVector<T, BASE>::dvlinfo()
{
  return s_info;
}


/**
 * @brief Return the DV/DL info struct for this class.
 *
 * This can be used to make sure that it's instantiated.
